public class Main {
    public static void main(String[] args) {
        double S=0.0;
        double C=0.0;
        double pi=3.14;
        S=pi*5.5*5.5;
        C=pi*2*5.5;
        System.out.println(S);
        System.out.println(C);
    }
}
